{
  "name": "yhzai-sa",
  "version": "1.0.0",
  "main": "dist/src/index.js",
  "typings": "dist/src/index.d.ts",
  "bin": {
    "nodejs-typescript-modern-starter": "dist/src/index.js",
    "ntms": "dist/src/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xddq/schema2typebox.git"
  },
  "keywords": [
    "nodejs",
    "typescript",
    "modern",
    "starter",
    "nodejs typescript modern starter",
    "debugging",
    "ts",
    "esbuild",
    "prettier",
    "typescript5",
    "CI/CD",
    "minimal"
  ],
  "author": "yhzai",
  "license": "MIT",
  "scripts": {
    "bundle": "rimraf dist && esbuild --outdir=dist --sourcemap --bundle --platform=node --target=node20.10.0 ./src/index.ts",
    "bundle:prod": "rimraf dist && esbuild --outdir=dist --bundle --platform=node --target=node20.10.0 ./src/index.ts",
    "compile": "tsc",
    "compile:watch": "tsc -w",
    "dev": "nodemon --watch src --watch test --ext ts,json --exec 'yarn bundle && yarn start'",
    "format": "prettier . --write",
    "format:check": "prettier . --check",
    "lint": "eslint . --fix",
    "lint:check": "eslint .",
    "start": "node --enable-source-maps ./dist/index.js",
    "test": "jest --runInBand .",
    "prettier-format": "prettier --config .prettierrc 'src/**/*.ts' --write",
    "prettier-watch": "onchange 'src/*.ts' -- prettier --write {{changed}}"
  },
  "devDependencies": {
    "@babel/core": "7.24.5",
    "@babel/preset-env": "7.24.5",
    "@babel/preset-typescript": "7.24.1",
    "@jest/globals": "29.7.0",
    "@tsconfig/node20": "20.1.4",
    "@types/body-parser": "^1.19.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "29.5.12",
    "@types/node": "^20.12.11",
    "@types/ws": "^8.5.10",
    "@typescript-eslint/eslint-plugin": "6.21.0",
    "@typescript-eslint/parser": "6.21.0",
    "esbuild": "0.21.3",
    "eslint": "8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "29.7.0",
    "nodemon": "3.1.0",
    "onchange": "^7.1.0",
    "prettier": "3.2.5",
    "rimraf": "5.0.7",
    "run-script-os": "^1.1.6",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5",
    "yarn": "^1.22.22"
  },
  "packageManager": "yarn@1.22.22",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.582.0",
    "@aws-sdk/client-secrets-manager": "^3.580.0",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "express": "^4.19.2",
    "fs": "^0.0.1-security",
    "node-cache": "^5.1.2",
    "openai": "^4.47.1",
    "path": "^0.12.7",
    "ws": "^8.17.0"
  }
}
